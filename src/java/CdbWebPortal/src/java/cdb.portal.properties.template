cdb.portal.adminGroupList=CDB_ADMIN

# 'USERNAME' string will be replaced by the actual username
cdb.portal.ldapUrl=ldaps://phoebusldap.aps.anl.gov:636
cdb.portal.ldapDnString=uid=USERNAME,ou=people,o=aps.anl.gov,dc=aps,dc=anl,dc=gov

# Storage areas
cdb.portal.storageDirectory=CDB_DATA_DIR
cdb.portal.scaledImageSize=450
cdb.portal.thumbnailImageSize=32

# Handlers
# HTTP link begin/end display length, e.g.: https://edp...12345.jpg
cdb.portal.handler.http.linkDisplayLength=32

# 'COLLECTION_ID' will be inserted
cdb.portal.handler.edp.urlString=https://edp.aps.anl.gov/browse/index/collection_id/COLLECTION_ID

# 'CONTENT_ID' will be inserted
cdb.portal.handler.icms.urlString=https://icmsdocs.aps.anl.gov/docs/idcplg?IdcService=DISPLAY_URL&dDocName=CONTENT_ID

# 'ORDER_ID' will be inserted
cdb.portal.handler.amos.urlString=https://apps.anl.gov/webcompadapter/viewrpt.cwr?id=10211&apsuser=irisuser&apspassword=irisuser2a&apsauthtype=secEnterprise&cmd=EXPORT&EXPORT_FMT=U2FPDF%3A0&promptex-ORDER_NO=ORDER_ID

# 'PO_ID' will be inserted
cdb.portal.handler.paris.urlString=https://apps.inside.anl.gov/paris/req.jsp?reqNbr=PO_ID

# CDB Web Service
cdb.webService.url=http://localhost:10232/cdb

# Traveler Web Service 
traveler.webSerice.url=http://127.0.0.1:3443
# Traveler Web Serivce basic auth credentials 
traveler.webService.basicAuth.username=api_write
traveler.webService.basicAuth.password=write
# Traveler Web Application
traveler.webApp.url=https://127.0.0.1:3001
# 'FORM_ID' will be inserted 
traveler.webApp.TemplatePath=/forms/FORM_ID/
# 'TRAVELER_ID' will be inserted
traveler.webApp.TravelerPath=/travelers/TRAVELER_ID/
# 'TRAVELER_ID' will be inserted
traveler.webApp.TravelerConfigPath=/travelers/TRAVELER_ID/config
